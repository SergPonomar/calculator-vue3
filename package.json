{
  "name": "calculator-vue3",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "preview": "vite preview",
    "lint": "eslint --ext .html,js,json,md,ts,vue --ignore-path .gitignore . --format tap",
    "test": "vitest run --environment jsdom",
    "test:e2e": "concurrently --kill-others \"npm run dev\" \"npm run cy:run\""
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@html-eslint/eslint-plugin": "^0.13.2",
    "@html-eslint/parser": "^0.13.2",
    "@rushstack/eslint-patch": "^1.1.3",
    "@testing-library/user-event": "^14.2.0",
    "@testing-library/vue": "^6.6.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^10.0.0",
    "concurrently": "^7.2.1",
    "cypress": "^10.0.3",
    "eslint": "^8.16.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-md": "^1.0.19",
    "jsdom": "^19.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.4",
    "vue-tsc": "^0.34.7"
  }
}
